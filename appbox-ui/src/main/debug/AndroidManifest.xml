<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.xiaoe.app2">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/abu_ic_launcher"
            android:label="@string/abu_app_name"
            android:roundIcon="@mipmap/abu_ic_launcher"
            android:name=".App"
            android:supportsRtl="true"
            android:usesCleartextTraffic="true"
            android:theme="@style/abu_AppTheme" tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>